unset CCACHE_DISABLE
export PATH="/usr/lib64/ccache/bin:$PATH"
export CCACHE_NOHASHDIR=true
export CCACHE_CPP2=true
export CCACHE_SLOPPINESS=modules,include_file_mtime,include_file_ctime,time_macros,pch_defines,file_stat_matches,clang_index_store,system_headers,locale
export CCACHE_DIR=/var/tmp/ccache
export CCACHE_BASEDIR=/builddir/build/BUILD
#
unset LDFLAGS
unset CFLAGS
unset CXXFLAGS
unset FCFLAGS
unset FFLAGS
#
export CFLAGS="-m32 -mstackrealign -fomit-frame-pointer -fexceptions -Wl,--as-needed -fno-lto -fuse-ld=bfd -fuse-linker-plugin -fno-lto -ffat-lto-objects -Wl,--build-id=sha1 -Wno-error -Wl,-z,max-page-size=0x1000 -O3 -march=native -mtune=native -Wall -Wl,--enable-new-dtags -falign-functions=32 -flimit-function-alignment -fasynchronous-unwind-tables -fdevirtualize-at-ltrans -floop-nest-optimize -ftree-loop-distribute-patterns -ftree-loop-vectorize -ftree-vectorize -funroll-loops -malign-data=cacheline -Wl,-sort-common -Wp,-D_REENTRANT -pipe -pthread -fPIC"
#
export CXXFLAGS="-m32 -mstackrealign -fomit-frame-pointer -fexceptions -Wl,--as-needed -fno-lto -fuse-ld=bfd -fuse-linker-plugin -fno-lto -ffat-lto-objects -Wl,--build-id=sha1 -Wno-error -Wl,-z,max-page-size=0x1000 -O3 -march=native -mtune=native -Wall -Wl,--enable-new-dtags -falign-functions=32 -flimit-function-alignment -fasynchronous-unwind-tables -fdevirtualize-at-ltrans -floop-nest-optimize -ftree-loop-distribute-patterns -ftree-loop-vectorize -ftree-vectorize -funroll-loops -malign-data=cacheline -Wl,-sort-common -Wp,-D_REENTRANT -fvisibility-inlines-hidden -pipe -pthread -fPIC"
#
export FCFLAGS="-m32 -mstackrealign -fomit-frame-pointer -fexceptions -Wl,--as-needed -fno-lto -fuse-ld=bfd -fuse-linker-plugin -fno-lto -ffat-lto-objects -Wl,--build-id=sha1 -Wno-error -Wl,-z,max-page-size=0x1000 -O3 -march=native -mtune=native -Wall -Wl,--enable-new-dtags -falign-functions=32 -flimit-function-alignment -fasynchronous-unwind-tables -fdevirtualize-at-ltrans -floop-nest-optimize -ftree-loop-distribute-patterns -ftree-loop-vectorize -ftree-vectorize -funroll-loops -malign-data=cacheline -Wl,-sort-common -Wp,-D_REENTRANT -pipe -pthread -fPIC"
#
export FFLAGS="-m32 -mstackrealign -fomit-frame-pointer -fexceptions -Wl,--as-needed -fno-lto -fuse-ld=bfd -fuse-linker-plugin -fno-lto -ffat-lto-objects -Wl,--build-id=sha1 -Wno-error -Wl,-z,max-page-size=0x1000 -O3 -march=native -mtune=native -Wall -Wl,--enable-new-dtags -falign-functions=32 -flimit-function-alignment -fasynchronous-unwind-tables -fdevirtualize-at-ltrans -floop-nest-optimize -ftree-loop-distribute-patterns -ftree-loop-vectorize -ftree-vectorize -funroll-loops -malign-data=cacheline -Wl,-sort-common -Wp,-D_REENTRANT -pipe -pthread -fPIC"
#
export LDFLAGS="-m32 -mstackrealign -fomit-frame-pointer -fexceptions -Wl,--as-needed -fno-lto -fuse-ld=bfd -fuse-linker-plugin -fno-lto -ffat-lto-objects -Wl,--build-id=sha1 -Wno-error -Wl,-z,max-page-size=0x1000 -O3 -march=native -mtune=native -Wall -Wl,--enable-new-dtags -falign-functions=32 -flimit-function-alignment -fasynchronous-unwind-tables -fdevirtualize-at-ltrans -floop-nest-optimize -ftree-loop-distribute-patterns -ftree-loop-vectorize -ftree-vectorize -funroll-loops -malign-data=cacheline -Wl,-sort-common -Wp,-D_REENTRANT -pipe -pthread -fPIC"
#
export V=1
export VERBOSE=1
export PKG_CONFIG_PATH="/usr/lib32/pkgconfig"
